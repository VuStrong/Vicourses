{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Fatal",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Fatal",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RabbitMQ": "amqp://user:pass@localhost:5672"
  },
  "CourseDB": {
    "Uri": "mongodb://localhost:27017",
    "DbName": "vicourses_course_db"
  },
  "FileUploadSecret": "jwt-secret-from-storage-service",
  "MediaFileSecret": "media-file-jwt-secret",
  "RabbitMqRetryDelay": 0
}
